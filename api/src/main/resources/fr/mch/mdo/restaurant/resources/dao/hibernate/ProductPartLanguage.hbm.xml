<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="fr.mch.mdo.restaurant.dao.beans.ProductPartLanguage" table="t_product_part_language">
		<id name="id" column="ppl_id" type="long">
			<generator class="increment" />
		</id>
		<property name="deleted" column="ppl_deleted" />
		<many-to-one name="locale" column="loc_id"
			class="fr.mch.mdo.restaurant.dao.beans.Locale" />
		<many-to-one name="productPart" column="prp_id"
			class="fr.mch.mdo.restaurant.dao.beans.ProductPart" />
		<property name="label" column="ppl_label" type="string" />
	</class>

</hibernate-mapping>
