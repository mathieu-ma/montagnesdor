<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="fr.mch.mdo.restaurant.dao.beans.CategoryLanguage" table="t_category_language">
		<id name="id" column="ctl_id" type="long">
			<generator class="increment" />
		</id>
		<property name="deleted" column="ctl_deleted" />
		<property name="label" column="ctl_label" />
		<property name="locId" column="loc_id" />
		<property name="parentId" column="cat_id" />
	</class>

</hibernate-mapping>
