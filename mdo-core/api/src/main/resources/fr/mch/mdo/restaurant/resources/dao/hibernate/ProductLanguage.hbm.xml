<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="fr.mch.mdo.restaurant.dao.beans.ProductLanguage" table="t_product_language">
		<id name="id" column="pdl_id" type="long">
			<generator class="increment" />
		</id>
		<property name="deleted" column="pdl_deleted" />
		<property name="label" column="pdl_label" />
		<property name="locId" column="loc_id" />
		<property name="parentId" column="pdt_id" />
	</class>

</hibernate-mapping>
