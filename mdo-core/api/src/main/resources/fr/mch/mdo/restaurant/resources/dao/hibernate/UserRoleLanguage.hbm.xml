<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="fr.mch.mdo.restaurant.dao.beans.UserRoleLanguage" table="t_user_role_language">
		<id name="id" column="url_id" type="long">
			<generator class="increment" />
		</id>
		<property name="deleted" column="url_deleted" />
		<property name="label" column="url_label" />
		<property name="locId" column="loc_id" />
		<property name="parentId" column="uro_id" />
	</class>
	

</hibernate-mapping>
