<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fr.mch.mdo.restaurant.dao.beans.Credit" table="t_credit">
		<id name="id" column="cre_id" type="long">
			<generator class="increment" />
		</id>
		<property name="reference" column="cre_reference" type="string" />
		<property name="amount" column="cre_amount" type="float" />
		<property name="deleted" column="cre_deleted" type="boolean" />
	</class>
</hibernate-mapping>
