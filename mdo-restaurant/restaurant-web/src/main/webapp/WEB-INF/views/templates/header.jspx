<?xml version="1.0" encoding="UTF-8"?>
<jsp:root version="2.1" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
	xmlns:spring="http://www.springframework.org/tags">
	<![CDATA[
		<script type="text/x-handlebars" data-template-name="application">
			<div id="header">
				<div id="header-resizable" class="mdo-resizable">{{outlet header}}</div>
			</div>
			<div id="menu">
				<div id="menu-resizable" class="mdo-resizable">{{outlet menu}}</div>
			</div>
			<div id="body">
				<div id="body-resizable" class="mdo-resizable">{{outlet body}}</div>
			</div>
			<div id="footer">
				<div id="footer-resizable" class="mdo-resizable">{{outlet footer}}</div>
			</div>
		</script>

		<script type="text/x-handlebars" data-template-name="header">
			<span id="header-languages">
				{{outlet headerLanguages}}
			</span>
			<span id="header-buttons">
				{{outlet headerButtons}}
			</span>
			<span id="header-order">
				{{outlet headerOrder}}
			</span>
		</script>
		<script type="text/x-handlebars" data-template-name="headerLanguages">
			{{#each language in content}}
				{{#with language}}
					{{#view "Mdo.FlagView" id=id languageIso2=languageIso2 displayLanguage=displayLanguage}}{{/view}}
				{{/with}}	
			{{/each}}
		</script>
		<script type="text/x-handlebars" data-template-name="headerButtons">
			{{#each button in content}}
				{{#with button}}
					{{#view "Mdo.ButtonView" labelKey=labelKey icons=icons selectedBinding="selected" click=click}}{{/view}}
				{{/with}}	
			{{/each}}
		</script>
		<script type="text/x-handlebars" data-template-name="headerOrder">
			{{outlet headerOrderNumber}}
			{{outlet headerOrderCustomersNumber}}
		</script>
		<script type="text/x-handlebars" data-template-name="headerOrderNumber">
			{{view "Mdo.LabelView" labelKey="header.order.number"}} {{view Ember.TextField name="number" elementId="number" class="ui-widget-content" valueBinding="number"}}
		</script>
		<script type="text/x-handlebars" data-template-name="headerOrderCustomersNumber">
			{{view "Mdo.LabelView" labelKey="header.order.customers.number"}} {{view Ember.TextField name="customersNumber" elementId="customersNumber" class="ui-widget-content" valueBinding="customersNumber"}}
		</script>
		
		<script type="text/x-handlebars" data-template-name="user">
			{{mma}}
			{{view "Mdo.LabelView" labelKey="header.access.locked.orders"}}
			{{view "Mdo.LabelView" labelKey="header.access.locked.orders"}}
			{{view "Mdo.LabelView" labelKey="header.access.locked.orders"}}
			{{view "Mdo.LabelView" labelKey="header.access.locked.orders"}}
			{{view "Mdo.LabelView" labelKey="header.access.locked.orders"}}
		</script>

		<script type="text/x-handlebars" data-template-name="orders">
			{{mma}}
		</script>

		<script type="text/x-handlebars" data-template-name="cashedOrders">
			{{mma}}
		</script>
		
		<script type="text/x-handlebars" data-template-name="lockedOrders">
			{{mma}}
		</script>
	]]>
</jsp:root>