<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

        <class name="fr.mch.mdo.restaurant.dao.beans.ProductLanguage" table="t_product_language">
        	<id name="id" column="pdl_id" type="long">
				<generator class="increment"/>
			</id>
			<property name="deleted" column="pdl_deleted"/>
			<many-to-one name="locale" column="loc_id" class="fr.mch.mdo.restaurant.dao.beans.Locale"/>			        
	        <many-to-one name="product" column="pdt_id" class="fr.mch.mdo.restaurant.dao.beans.Product"/>
            <property name="label" column="pdl_label" type="string"/>
        </class>

</hibernate-mapping>
